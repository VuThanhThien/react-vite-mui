{
  "name": "react-vite-mui",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": "=18.19.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:staging": "env-cmd -f .env.staging vite build --mode staging",
    "build:production": "env-cmd -f .env.production vite build --mode production",
    "predeploy": "yarn run build:production",
    "deploy": "gh-pages -d build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format:all": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.15.3",
    "@mui/lab": "^5.0.0-alpha.159",
    "@mui/material": "^5.15.3",
    "@mui/styled-engine-sc": "^6.0.0-alpha.11",
    "@mui/styles": "^5.15.3",
    "@mui/x-date-pickers": "^6.18.7",
    "@react-oauth/google": "^0.12.1",
    "@sentry/react": "^7.93.0",
    "apexcharts": "^3.45.1",
    "axios": "^1.6.5",
    "chroma-js": "^2.4.2",
    "date-fns": "^3.1.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "formik": "^2.4.5",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-xhr-backend": "^3.2.2",
    "jwt-decode": "^4.0.0",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "query-string": "^8.1.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-apexcharts": "^1.4.1",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-error-boundary": "^4.0.12",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.49.2",
    "react-i18next": "^14.0.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.21.1",
    "react-spinners": "^0.13.8",
    "rollup-plugin-visualizer": "^5.12.0",
    "styled-components": "^6.1.7",
    "uuid": "^9.0.1",
    "vite-tsconfig-paths": "^4.2.3",
    "web-vitals": "^1.0.1",
    "yup": "^1.3.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.4.3",
    "@types/chrome": "^0.0.256",
    "@types/node": "^20.10.7",
    "@types/nprogress": "^0.2.3",
    "@types/numeral": "^2.0.5",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "7.0.4",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,json,scss,md,html}": [
      "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
      "git add ."
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "env": {
      "browser": true,
      "node": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
